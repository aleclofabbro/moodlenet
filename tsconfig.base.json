{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "jsx": "preserve",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "noUncheckedIndexedAccess": true,
    "esModuleInterop": true,
    "strict": true,
    "paths": {
      "@moodle/bindings-node": ["bindings/node/src/index.ts"],
      "@moodle/lib-ddd": ["lib/ddd/src/index.ts"],
      "@moodle/lib-email-templates/iam": ["lib/email-templates/src/moodle/iam/index.ts"],
      "@moodle/lib-email-templates/org": ["lib/email-templates/src/moodle/org/index.ts"],
      "@moodle/lib-id-gen": ["lib/id-gen/src/index.ts"],
      "@moodle/lib-jwt-jose": ["lib/jwt/jose/src/index.ts"],
      "@moodle/lib-types": ["lib/types/src/index.ts"],
      "@moodle/mod-iam": ["mod/iam/src/index.ts"],
      "@moodle/mod-iam/lib": ["mod/iam/src/lib/index.ts"],
      "@moodle/mod-iam/setup": ["mod/iam/src/setup/index.ts"],
      "@moodle/mod-iam/types": ["mod/iam/src/types/index.ts"],
      "@moodle/mod-net": ["mod/net/src/index.ts"],
      "@moodle/mod-net-webapp-nextjs": ["mod/net-webapp-nextjs/src/index.ts"],
      "@moodle/mod-net-webapp-nextjs/lib": ["mod/net-webapp-nextjs/src/lib/index.ts"],
      "@moodle/mod-net-webapp-nextjs/setup": ["mod/net-webapp-nextjs/src/setup/index.ts"],
      "@moodle/mod-net-webapp-nextjs/types": ["mod/net-webapp-nextjs/src/types/index.ts"],
      "@moodle/mod-net/lib": ["mod/net/src/lib/index.ts"],
      "@moodle/mod-net/setup": ["mod/net/src/setup/index.ts"],
      "@moodle/mod-net/types": ["mod/net/src/types/index.ts"],
      "@moodle/mod-org": ["mod/org/src/index.ts"],
      "@moodle/mod-org/lib": ["mod/org/src/lib/index.ts"],
      "@moodle/mod-org/setup": ["mod/org/src/setup/index.ts"],
      "@moodle/mod-org/types": ["mod/org/src/types/index.ts"],
      "@moodle/sec-crypto-default": ["sec/crypto/default/src/index.ts"],
      "@moodle/sec-db-arango": ["sec/db/arango/src/index.ts"],
      "@moodle/sec-db-arango/migrate": ["sec/db/arango/src/migrate.ts"],
      "@moodle/sec-email-nodemailer": ["sec/email/nodemailer/src/index.ts"],
      "@moodle/sec-files-minio": ["sec/files/minio/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
