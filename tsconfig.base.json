{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "jsx": "preserve",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "noUncheckedIndexedAccess": true,
    "esModuleInterop": true,
    "strict": true,
    "paths": {
      "@moodle/bindings-node": ["bindings/node/src/index.ts"],
      "@moodle/domain": ["domain/src/index.ts"],
      "@moodle/lib-domain": ["lib/domain/src/index.ts"],
      "@moodle/lib-email-templates": ["lib/email-templates/src/index.ts"],
      "@moodle/lib-id-gen": ["lib/id-gen/src/index.ts"],
      "@moodle/lib-jwt-jose": ["lib/jwt/jose/src/index.ts"],
      "@moodle/lib-types": ["lib/types/src/index.ts"],
      "@moodle/mod-iam/v1_0_setup": ["mod/iam/src/v1_0/setup/index.ts"],
      "@moodle/mod-net/v1_0_setup": ["mod/net/src/v1_0/setup/index.ts"],
      "@moodle/mod-net-webapp-nextjs/v1_0_setup": ["mod/net-webapp-nextjs/src/v1_0/setup/index.ts"],
      "@moodle/mod-org/v1_0_setup": ["mod/org/src/v1_0/setup/index.ts"],
      "@moodle/mod-iam": ["mod/iam/src/index.ts"],
      "@moodle/mod-net": ["mod/net/src/index.ts"],
      "@moodle/mod-net-webapp-nextjs": ["mod/net-webapp-nextjs/src/index.ts"],
      "@moodle/mod-org": ["mod/org/src/index.ts"],
      "@moodle/sec-crypto-default": ["sec/crypto/default/src/index.ts"],
      "@moodle/sec-db-arango": ["sec/db/arango/src/index.ts"],
      "@moodle/sec-db-arango/migrate": ["sec/db/arango/src/migrate.ts"],
      "@moodle/sec-email-nodemailer": ["sec/email/nodemailer/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
